{
  "name": "@sidedrawer/sdk",
  "version": "0.0.1",
  "description": "SideDrawer SDK",
  "source": "src/index.ts",
  "module": "dist/sidedrawer.mjs",
  "main": "dist/sidedrawer.js",
  "types": "dist/types.d.ts",
  "min": "dist/sidedrawer.min.js",
  "sourceMap": false,
  "targets": {
    "module": {
      "sourceMap": false,
      "optimize": true
    },
    "main": {
      "sourceMap": false,
      "optimize": true
    },
    "types": {
      "sourceMap": false
    },
    "min": {
      "sourceMap": false,
      "context": "browser",
      "engines": {
        "browsers": "> 0.2%, not dead"
      },
      "outputFormat": "global",
      "scopeHoist": true,
      "optimize": true
    }
  },
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "jest --runInBand --coverage",
    "build-docs": "npx typedoc --options typedoc.json"
  },
  "author": "SideDrawer Inc.",
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@swc/helpers": "^0.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "buffer": "^6.0.3",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.1",
    "typedoc-plugin-rename-defaults": "^0.6.5",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "rxjs": "^7.8.0"
  }
}
